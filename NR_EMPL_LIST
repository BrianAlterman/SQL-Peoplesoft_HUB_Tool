	
SELECT A.EMPLID, A.NAME, E.AC_ACQUISITION_COD, TO_CHAR(A.HIRE_DT,'YYYY-MM-DD'), TO_CHAR(A.REHIRE_DT,'YYYY-MM-DD'), TO_CHAR(A.SERVICE_DT,'YYYY-MM-DD'), A.SUPERVISOR_ID, A.AC_SUPER_NAME, A.JOBCODE, A.FLSA_STATUS, A.EMPL_STATUS, A.ACTION, TO_CHAR(A.ACTION_DT,'YYYY-MM-DD'), A.ACTION_REASON, A.FULL_PART_TIME, A.STD_HOURS, A.REVIEW_RATING, TO_CHAR(A.REVIEW_DT,'YYYY-MM-DD'), A.ANNUAL_RT, A.HOURLY_RT, B.DESCR, B.GRADE, TO_CHAR(A.JOB_ENTRY_DT,'YYYY-MM-DD'), C.AC_EXT_LVL01_NAME, C.AC_EXT_LVL02_NAME, C.AC_EXT_LVL03_NAME, A.LOCATION, D.DESCR, A.LABOR_TYPE_GER, A.AC_BUSINESS_UNIT, A.AC_EMAIL
  FROM PS_AC_EMPLS_VW A, PS_EMPLMT_SRCH_QRY A1, PS_JOBCODE_TBL B, PS_SET_CNTRL_REC B2, PS_AC_REPTS_TO_LVL C, PS_SET_LOC_VW D, PS_EMPLOYMENT E, PS_EMPLMT_SRCH_QRY E1
  WHERE ( B.JOBCODE = A.JOBCODE
    AND B2.SETCNTRLVALUE = A.BUSINESS_UNIT
    AND B2.RECNAME = 'JOBCODE_TBL'
    AND B2.SETID = B.SETID
    AND D.LOCATION = A.LOCATION
    AND A.EMPLID = A1.EMPLID
    AND A1.OPRID = 'E0182265'
    AND E.EMPLID = E1.EMPLID
    AND E.EMPL_RCD = E1.EMPL_RCD
    AND E1.OPRID = 'E0182265'
    AND ( A.EFFDT =
        (SELECT MAX(A_ED.EFFDT) FROM PS_AC_EMPLS_VW A_ED
        WHERE A.EMPLID = A_ED.EMPLID
          AND A_ED.EFFDT <= SYSDATE)
    AND A.EFFSEQ =
        (SELECT MAX(A_ES.EFFSEQ) FROM PS_AC_EMPLS_VW A_ES
        WHERE A.EMPLID = A_ES.EMPLID
          AND A.EFFDT = A_ES.EFFDT)
     AND B.EFFDT =
        (SELECT MAX(B_ED.EFFDT) FROM PS_JOBCODE_TBL B_ED
        WHERE B.SETID = B_ED.SETID
          AND B.JOBCODE = B_ED.JOBCODE
          AND B_ED.EFFDT <= A.EFFDT)
     AND A.EMPLID = C.EMPLID
     AND C.POSITION_NBR = A.POSITION_NBR
     AND A.EMPL_STATUS IN ('A','L','P')
     AND D.EFFDT =
        (SELECT MAX(D_ED.EFFDT) FROM PS_SET_LOC_VW D_ED
        WHERE D.SETID_LOCATION = D_ED.SETID_LOCATION
          AND D.LOCATION = D_ED.LOCATION
          AND D_ED.EFFDT <= A.EFFDT)
     AND A.EMPLID = E.EMPLID ))
