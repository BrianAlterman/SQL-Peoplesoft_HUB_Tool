	
SELECT D.EMPLID, D.NAME, D.POSITION_NBR, E.COMPANY, D.DEPTID, D.DEPT_DESCR, D.JOBCODE, D.JOBCODE_DESCR, D.LOCATION, D.LOCATION_DESCR, D.REPORTS_TO, D.SUPERVISOR_ID, D.AC_SUPER_NAME, F.AC_EXT_LVL00_NAME, F.AC_EXT_LVL01_NAME, F.AC_EXT_LVL02_NAME, F.AC_EXT_LVL03_NAME, F.AC_EXT_LVL04_NAME, F.AC_EXT_LVL05_NAME
  FROM PS_PSTREENODE_DW A, PS_AC_NSPOS_INC_VW D, PS_AC_POSN_SRCH D1, PS_POSITION_DATA E, PS_POSTN_SRCH_QRY E1, PS_AC_REPTS_TO_LVL F
  WHERE ( D.POSITION_NBR = D1.POSITION_NBR
    AND D1.OPRCLASS = 'WIN_VW'
    AND E.POSITION_NBR = E1.POSITION_NBR
    AND E1.OPRID = 'E0182265'
    AND ( A.TREE_NODE_NUM >= (SELECT B.TREE_NODE_NUM
  FROM PS_PSTREENODE_DW B
  WHERE B.TREE_NODE = :1)
     AND A.TREE_NODE_NUM_END <= (SELECT C.TREE_NODE_NUM_END
  FROM PS_PSTREENODE_DW C
  WHERE C.TREE_NODE = :1)
     AND D.POSITION_NBR = A.TREE_NODE
     AND A.SETID = 'COMMN'
     AND D.EMPLID = ' '
     AND D.POSITION_NBR = E.POSITION_NBR
     AND E.EFFDT =
        (SELECT MAX(E_ED.EFFDT) FROM PS_POSITION_DATA E_ED
        WHERE E.POSITION_NBR = E_ED.POSITION_NBR
          AND E_ED.EFFDT <= D.EFFDT)
     AND D.POSITION_NBR = F.POSITION_NBR
     AND D.EMPLID = F.EMPLID ))
  ORDER BY 11
