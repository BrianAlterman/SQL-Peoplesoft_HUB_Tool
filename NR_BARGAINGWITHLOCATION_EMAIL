SELECT A.DIST_ID, A.UNION_CD, C.DESCR, B.BARG_UNIT, B.DESCR, B.AC_GOV_LOCAL, B.CONTRACT_NUM, A.EMPLID, A.NAME, A.ADDRESS1, A.ADDRESS2, A.CITY, A.STATE, A.POSTAL, A.EMPL_STATUS, A.EMPL_TYPE, TO_CHAR(A.HIRE_DT,'YYYY-MM-DD'), TO_CHAR(A.REHIRE_DT,'YYYY-MM-DD'), TO_CHAR(A.SERVICE_DT,'YYYY-MM-DD'), TO_CHAR(A.CMPNY_SENIORITY_DT,'YYYY-MM-DD'), TO_CHAR(A.UNION_SENIORITY_DT,'YYYY-MM-DD'), A.JOBCODE, A.JOBTITLE, TO_CHAR(A.JOB_ENTRY_DT,'YYYY-MM-DD'), A.SEX, A.ETHNIC_GROUP, TO_CHAR(A.BIRTHDATE,'YYYY-MM-DD'), A.HOURLY_RT, A.ANNUAL_RT, A.FLSA_STATUS, A.LMRA_CD, TO_CHAR(A.LMRA_DT,'YYYY-MM-DD'), A.LOCATION, A.LOCATION_NAME, A.AC_SUPER_NAME, A.DEPTID, TO_CHAR(A.DEPT_ENTRY_DT,'YYYY-MM-DD'), A.HOME_PHONE, E.AC_WAGE_SERV_TYPE, TO_CHAR(E.NEXT_SERVICE_DT,'YYYY-MM-DD'), G.E_ADDR_TYPE, G.EMAIL_ADDR, TO_CHAR(A.BIRTHDATE,'YYYY-MM-DD')
  FROM PS_AC_EMPLS_VW A, PS_EMPLMT_SRCH_QRY A1, PS_AC_BARG_UNIT B, PS_UNION_TBL C, PS_JOB_LABOR E, PS_UNION_TBL F, PS_EMAIL_ADDRESSES G, PS_PERALL_SEC_QRY G1
  WHERE ( F.UNION_CD = A.UNION_CD
    AND A.EMPLID = A1.EMPLID
    AND A1.OPRID = 'E0182265'
    AND G.EMPLID = G1.EMPLID
    AND G1.OPRID = 'E0182265'
    AND ( A.EFFDT =
        (SELECT MAX(A_ED.EFFDT) FROM PS_AC_EMPLS_VW A_ED
        WHERE A.EMPLID = A_ED.EMPLID
          AND A_ED.EFFDT <= SYSDATE)
    AND A.EFFSEQ =
        (SELECT MAX(A_ES.EFFSEQ) FROM PS_AC_EMPLS_VW A_ES
        WHERE A.EMPLID = A_ES.EMPLID
          AND A.EFFDT = A_ES.EFFDT)
     AND B.UNION_CD = A.UNION_CD
     AND B.BARG_UNIT = A.BARG_UNIT
     AND B.EFFDT =
        (SELECT MAX(B_ED.EFFDT) FROM PS_AC_BARG_UNIT B_ED
        WHERE B.UNION_CD = B_ED.UNION_CD
          AND B.BARG_UNIT = B_ED.BARG_UNIT
          AND B_ED.EFFDT <= SYSDATE)
     AND C.UNION_CD = A.UNION_CD
     AND C.EFFDT =
        (SELECT MAX(C_ED.EFFDT) FROM PS_UNION_TBL C_ED
        WHERE C.UNION_CD = C_ED.UNION_CD
          AND C_ED.EFFDT <= SYSDATE)
     AND A.EMPLID = E.EMPLID
     AND A.EFFSEQ = E.EFFSEQ
     AND E.EFFDT =
        (SELECT MAX(E_ED.EFFDT) FROM PS_JOB_LABOR E_ED
        WHERE E.EMPLID = E_ED.EMPLID
          AND E.EMPL_RCD = E_ED.EMPL_RCD
          AND E_ED.EFFDT <= SYSDATE)
    AND E.EFFSEQ =
        (SELECT MAX(E_ES.EFFSEQ) FROM PS_JOB_LABOR E_ES
        WHERE E.EMPLID = E_ES.EMPLID
          AND E.EMPL_RCD = E_ES.EMPL_RCD
          AND E.EFFDT = E_ES.EFFDT)
     AND F.EFFDT =
        (SELECT MAX(F_ED.EFFDT) FROM PS_UNION_TBL F_ED
        WHERE F.UNION_CD = F_ED.UNION_CD
          AND F_ED.EFFDT <= A.EFFDT)
     AND A.EMPLID = G.EMPLID ))
  ORDER BY 1, 2, 4, 9
