	
SELECT B.EMPLID, B.NAME, B.SEX, B.ETHNIC_GROUP, B.JOBCODE, B.JOBTITLE, B.EEO1CODE, B.AC_CAT05, B.AC_CAT05_DESCR, 
B.LOCATION, B.FULL_PART_TIME, B.LOCATION_NAME, B.STATE, TO_CHAR(B.SERVICE_DT,'YYYY-MM-DD'), TO_CHAR(B.BIRTHDATE,'YYYY-MM-DD'), 
B.COMPANY, B.DISABLED, B.DISABLED_VET, B.AC_SUPER_NAME, B.GRADE, B.WIN_MILITARY_STAT, TO_CHAR(SYSDATE,'YYYY-MM-DD'), 
TO_CHAR(SYSDATE,'YYYY-MM-DD'), TO_CHAR(SYSDATE,'YYYY-MM-DD')
  FROM PS_AC_EMPLS_VW B, PS_EMPLMT_SRCH_QRY B1
  WHERE ( B.EMPLID = B1.EMPLID
    AND B1.OPRID = 'E0182265')
