SELECT A.POSITION_NBR, A.JOBCODE, A.DESCR, A.DEPTID, C.DESCR, A.LOCATION, D.DESCR, A.REPORTS_TO, B.EMPLID, B.NAME, B.AC_EXT_LVL01_NAME, B.AC_EXT_LVL02_NAME, B.AC_EXT_LVL03_NAME, B.AC_EXT_LVL04_NAME, B.AC_EXT_LVL05_NAME, B.AC_LEVEL01_POSN, B.AC_LEVEL01_EMPLID, B.AC_LEVEL02_POSN, B.AC_LEVEL02_EMPLID, B.AC_LEVEL03_POSN, B.AC_LEVEL03_EMPLID, B.AC_LEVEL04_POSN, B.AC_LEVEL04_EMPLID, B.AC_LEVEL05_POSN, B.AC_LEVEL05_EMPLID, B.AC_LEVEL06_POSN, B.AC_LEVEL06_EMPLID, B.AC_EXT_LVL06_NAME, B.AC_LEVEL07_POSN, B.AC_LEVEL07_EMPLID, B.AC_EXT_LVL07_NAME, B.AC_LEVEL08_POSN, B.AC_LEVEL08_EMPLID, B.AC_EXT_LVL08_NAME, A.AC_BUSINESS_UNIT, A.UNION_CD, A.BARG_UNIT
  FROM PS_POSITION_DATA A, PS_POSTN_SRCH_QRY A1, PS_AC_REPTS_TO_LVL B, PS_DEPT_TBL C, PS_SET_CNTRL_REC C2, PS_LOCATION_TBL D, PS_SET_CNTRL_REC D2
  WHERE ( C.DEPTID = A.DEPTID
    AND C2.SETCNTRLVALUE = A.BUSINESS_UNIT
    AND C2.RECNAME = 'DEPT_TBL'
    AND C2.SETID = C.SETID
    AND D.LOCATION = A.LOCATION
    AND D2.SETCNTRLVALUE = A.BUSINESS_UNIT
    AND D2.RECNAME = 'LOCATION_TBL'
    AND D2.SETID = D.SETID
    AND A.POSITION_NBR = A1.POSITION_NBR
    AND A1.OPRID = 'E0182265'
    AND ( A.EFFDT =
        (SELECT MAX(A_ED.EFFDT) FROM PS_POSITION_DATA A_ED
        WHERE A.POSITION_NBR = A_ED.POSITION_NBR
          AND A_ED.EFFDT <= SYSDATE)
     AND A.AC_NON_EMPL_POS = 'N'
     AND A.EFF_STATUS = 'A'
     AND A.REPORTS_TO = B.POSITION_NBR
     AND C.EFFDT =
        (SELECT MAX(C_ED.EFFDT) FROM PS_DEPT_TBL C_ED
        WHERE C.SETID = C_ED.SETID
          AND C.DEPTID = C_ED.DEPTID
          AND C_ED.EFFDT <= SYSDATE)
     AND D.EFFDT =
        (SELECT MAX(D_ED.EFFDT) FROM PS_LOCATION_TBL D_ED
        WHERE D.SETID = D_ED.SETID
          AND D.LOCATION = D_ED.LOCATION
          AND D_ED.EFFDT <= SYSDATE) ))
