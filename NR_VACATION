SELECT A.EMPLID, A.COMPANY, A.PLAN_TYPE, TO_CHAR(A.ACCRUAL_PROC_DT,'YYYY-MM-DD'), A.HRS_EARNED_YTD, A.HRS_TAKEN_YTD, A.HRS_ADJUST_YTD, A.HRS_CARRYOVER, TO_CHAR(A.ACCRUAL_PROC_DT,'YYYY-MM-DD')
  FROM PS_LEAVE_ACCRUAL A, PS_EMPLMT_SRCH_QRY A1
  WHERE ( A.EMPLID = A1.EMPLID
    AND A.EMPL_RCD = A1.EMPL_RCD
    AND A1.OPRID = 'E0182265'
    AND ( A.PLAN_TYPE IN ('51','5Z','5Y')
     AND A.ACCRUAL_PROC_DT = TO_DATE('2017-01-21','YYYY-MM-DD') ))
