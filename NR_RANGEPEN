	
SELECT G.AC_ACQUISITION_COD, B.POSITION_NBR, A.EMPLID, A.NAME, B.EMPL_STATUS, TO_CHAR(B.HIRE_DT,'YYYY-MM-DD'), TO_CHAR(B.LAST_HIRE_DT,'YYYY-MM-DD'), TO_CHAR(B.JOB_ENTRY_DT,'YYYY-MM-DD'), TO_CHAR(B.POSITION_ENTRY_DT,'YYYY-MM-DD'), B.JOBCODE, C.GRADE, C.DESCR, F.DESCR, F.CITY, F.STATE, B.STD_HOURS, B.ANNUAL_RT, B.HOURLY_RT, B.SAL_ADMIN_PLAN, D.MIN_RT_ANNUAL, D.MID_RT_ANNUAL, D.MAX_RT_ANNUAL, B.AC_RANGEPEN, E.AC_EXT_LVL00_NAME, E.AC_EXT_LVL01_NAME, E.AC_EXT_LVL02_NAME, E.AC_EXT_LVL03_NAME, E.AC_EXT_LVL04_NAME, B.SUPERVISOR_ID, B.HR_STATUS, B.ACTION, TO_CHAR(B.ACTION_DT,'YYYY-MM-DD'), B.ACTION_REASON, B.CHANGE_AMT, B.CHANGE_PCT, B.REG_TEMP, B.FULL_PART_TIME
  FROM PS_PERSONAL_DATA A, PS_PERALL_SEC_QRY A1, PS_JOB B, PS_EMPLMT_SRCH_QRY B1, PS_JOBCODE_TBL C, PS_SET_CNTRL_REC C2, PS_SAL_GRADE_TBL D, PS_AC_REPTS_TO_LVL E, PS_LOCATION_TBL F, PS_EMPLOYMENT G, PS_EMPLMT_SRCH_QRY G1
  WHERE ( C.JOBCODE = B.JOBCODE
    AND C2.SETCNTRLVALUE = B.BUSINESS_UNIT
    AND C2.RECNAME = 'JOBCODE_TBL'
    AND C2.SETID = C.SETID
    AND A.EMPLID = A1.EMPLID
    AND A1.OPRID = 'E0182265'
    AND B.EMPLID = B1.EMPLID
    AND B.EMPL_RCD = B1.EMPL_RCD
    AND B1.OPRID = 'E0182265'
    AND G.EMPLID = G1.EMPLID
    AND G.EMPL_RCD = G1.EMPL_RCD
    AND G1.OPRID = 'E0182265'
    AND ( A.EMPLID = B.EMPLID
     AND B.EFFDT =
        (SELECT MAX(B_ED.EFFDT) FROM PS_JOB B_ED
        WHERE B.EMPLID = B_ED.EMPLID
          AND B.EMPL_RCD = B_ED.EMPL_RCD
          AND B_ED.EFFDT <= SYSDATE)
    AND B.EFFSEQ =
        (SELECT MAX(B_ES.EFFSEQ) FROM PS_JOB B_ES
        WHERE B.EMPLID = B_ES.EMPLID
          AND B.EMPL_RCD = B_ES.EMPL_RCD
          AND B.EFFDT = B_ES.EFFDT)
     AND B.EMPL_STATUS IN ('A','P','L')
     AND C.EFFDT =
        (SELECT MAX(C_ED.EFFDT) FROM PS_JOBCODE_TBL C_ED
        WHERE C.SETID = C_ED.SETID
          AND C.JOBCODE = C_ED.JOBCODE
          AND C_ED.EFFDT <= B.EFFDT)
     AND D.SAL_ADMIN_PLAN = B.SAL_ADMIN_PLAN
     AND D.GRADE = B.GRADE
     AND D.EFFDT =
        (SELECT MAX(D_ED.EFFDT) FROM PS_SAL_GRADE_TBL D_ED
        WHERE D.SETID = D_ED.SETID
          AND D.SAL_ADMIN_PLAN = D_ED.SAL_ADMIN_PLAN
          AND D.GRADE = D_ED.GRADE
          AND D_ED.EFFDT <= SYSDATE)
     AND B.EMPLID = E.EMPLID
     AND E.POSITION_NBR = B.POSITION_NBR
     AND A.EMPLID NOT LIKE 'V%'
     AND F.LOCATION = B.LOCATION
     AND F.EFFDT =
        (SELECT MAX(F_ED.EFFDT) FROM PS_LOCATION_TBL F_ED
        WHERE F.SETID = F_ED.SETID
          AND F.LOCATION = F_ED.LOCATION
          AND F_ED.EFFDT <= SYSDATE)
     AND A.EMPLID = G.EMPLID ))
