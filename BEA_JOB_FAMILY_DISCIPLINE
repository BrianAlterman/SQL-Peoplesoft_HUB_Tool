SELECT D.POSITION_NBR, D.EMPLID, D.NAME, TO_CHAR(D.SERVICE_DT,'YYYY-MM-DD'), E.AC_EMAIL, D.JOBCODE, D.JOBCODE_DESCR, H.JOB_FAMILY, I.DESCR, I.AC_FAMILY, J.DESCR, D.LOCATION, D.LOCATION_DESCR, D.AC_BUSINESS_UNIT, D.AC_JDE_DESCR, D.REPORTS_TO, D.SUPERVISOR_ID, D.AC_SUPER_NAME, F.AC_LEVEL01_POSN, F.AC_LEVEL01_EMPLID, F.AC_EXT_LVL01_NAME, F.AC_LEVEL01_BU, F.AC_LEVEL02_POSN, F.AC_LEVEL02_EMPLID, F.AC_EXT_LVL02_NAME, F.AC_LEVEL02_BU, F.AC_LEVEL03_POSN, F.AC_LEVEL03_EMPLID, F.AC_EXT_LVL03_NAME, F.AC_LEVEL03_BU, F.AC_LEVEL04_POSN, F.AC_LEVEL04_EMPLID, F.AC_EXT_LVL04_NAME, F.AC_LEVEL04_BU, F.AC_LEVEL05_POSN, F.AC_LEVEL05_EMPLID, F.AC_EXT_LVL05_NAME, F.AC_LEVEL05_BU, F.AC_LEVEL06_POSN, F.AC_LEVEL06_EMPLID, F.AC_EXT_LVL06_NAME, F.AC_LEVEL06_BU, F.AC_LEVEL07_POSN, F.AC_LEVEL07_EMPLID, F.AC_EXT_LVL07_NAME, F.AC_LEVEL07_BU, F.AC_LEVEL08_POSN, F.AC_LEVEL08_EMPLID, F.AC_EXT_LVL08_NAME, F.AC_LEVEL08_BU, G.AC_ACQUISITION_COD
  FROM PS_PSTREENODE_DW A, PS_AC_NSPOS_INC_VW D, PS_AC_POSN_SRCH D1, PS_AC_EMAIL_TBL E, PS_AC_REPTS_TO_LVL F, PS_WIN_EMPLOYMENT G, PS_AC_EMPLS_VW H, PS_EMPLMT_SRCH_QRY H1, PS_JOB_FAMILY_TBL I, PS_AC_FAMILY_TBL J
  WHERE ( D.POSITION_NBR = D1.POSITION_NBR
    AND D1.OPRCLASS = 'WIN_VW'
    AND H.EMPLID = H1.EMPLID
    AND H1.OPRID = 'E0182265'
    AND ( A.TREE_NODE_NUM >= (SELECT B.TREE_NODE_NUM
  FROM PS_PSTREENODE_DW B
  WHERE B.TREE_NODE = :1)
     AND A.TREE_NODE_NUM_END <= (SELECT C.TREE_NODE_NUM_END
  FROM PS_PSTREENODE_DW C
  WHERE C.TREE_NODE = :1)
     AND D.POSITION_NBR = A.TREE_NODE
     AND E.EMPLID = D.EMPLID
     AND F.EMPLID = D.EMPLID
     AND F.POSITION_NBR = D.POSITION_NBR
     AND G.EMPLID = D.EMPLID
     AND G.EMPLID = H.EMPLID
     AND H.EFFDT =
        (SELECT MAX(H_ED.EFFDT) FROM PS_AC_EMPLS_VW H_ED
        WHERE H.EMPLID = H_ED.EMPLID
          AND H_ED.EFFDT <= SYSDATE)
    AND H.EFFSEQ =
        (SELECT MAX(H_ES.EFFSEQ) FROM PS_AC_EMPLS_VW H_ES
        WHERE H.EMPLID = H_ES.EMPLID
          AND H.EFFDT = H_ES.EFFDT)
     AND I.JOB_FAMILY = H.JOB_FAMILY
     AND I.EFFDT =
        (SELECT MAX(I_ED.EFFDT) FROM PS_JOB_FAMILY_TBL I_ED
        WHERE I.JOB_FAMILY = I_ED.JOB_FAMILY
          AND I_ED.EFFDT <= H.EFFDT)
     AND J.AC_FAMILY = I.AC_FAMILY
     AND J.EFFDT =
        (SELECT MAX(J_ED.EFFDT) FROM PS_AC_FAMILY_TBL J_ED
        WHERE J.AC_FAMILY = J_ED.AC_FAMILY
          AND J_ED.EFFDT <= I.EFFDT) ))
