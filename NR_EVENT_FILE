	
SELECT B.AC_EXT_LVL01_NAME, B.AC_EXT_LVL02_NAME, B.AC_EXT_LVL03_NAME, B.AC_EXT_LVL04_NAME, B.AC_EXT_LVL05_NAME, B.AC_EXT_LVL06_NAME, A.EMPLID, A.NAME, A.POSITION_NBR, A.JOBTITLE, A.AC_SUPER_NAME, A.AC_BUSINESS_UNIT, A.AC_JDE_DESCR, A.LOCATION_NAME, F.NAME_AC
  FROM ((PS_AC_EMPLS_VW A INNER JOIN PS_EMPLMT_SRCH_QRY A1 ON (A.EMPLID = A1.EMPLID AND  A1.OPRID = 'E0182265' )) LEFT OUTER JOIN  PS_WIN_HRBP_EMP_VW F ON  A.EMPLID = F.EMPLID ), PS_AC_REPTS_TO_LVL B, PS_WIN_EMPLOYMENT C, PS_LOCATION_TBL D, PS_JOBCODE_TBL E
  WHERE ( ( A.EFFDT =
        (SELECT MAX(A_ED.EFFDT) FROM PS_AC_EMPLS_VW A_ED
        WHERE A.EMPLID = A_ED.EMPLID
          AND A_ED.EFFDT <= SYSDATE)
    AND A.EFFSEQ =
        (SELECT MAX(A_ES.EFFSEQ) FROM PS_AC_EMPLS_VW A_ES
        WHERE A.EMPLID = A_ES.EMPLID
          AND A.EFFDT = A_ES.EFFDT)
     AND A.EMPLID = B.EMPLID
     AND A.EMPLID = C.EMPLID
     AND D.LOCATION = A.LOCATION
     AND D.EFFDT =
        (SELECT MAX(D_ED.EFFDT) FROM PS_LOCATION_TBL D_ED
        WHERE D.SETID = D_ED.SETID
          AND D.LOCATION = D_ED.LOCATION
          AND D_ED.EFFDT <= SYSDATE)
     AND E.JOBCODE = A.JOBCODE
     AND E.EFFDT =
        (SELECT MAX(E_ED.EFFDT) FROM PS_JOBCODE_TBL E_ED
        WHERE E.SETID = E_ED.SETID
          AND E.JOBCODE = E_ED.JOBCODE
          AND E_ED.EFFDT <= SYSDATE) ))
