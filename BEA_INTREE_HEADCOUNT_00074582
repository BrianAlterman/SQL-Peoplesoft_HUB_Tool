SELECT D.POSITION_NBR, D.EMPLID, D.NAME, TO_CHAR(D.SERVICE_DT,'YYYY-MM-DD'), E.AC_EMAIL, D.JOBCODE, D.JOBCODE_DESCR, D.LOCATION, D.LOCATION_DESCR, D.AC_BUSINESS_UNIT, D.AC_JDE_DESCR, D.REPORTS_TO, D.SUPERVISOR_ID, D.AC_SUPER_NAME, F.AC_LEVEL01_POSN, F.AC_LEVEL01_EMPLID, F.AC_EXT_LVL01_NAME, F.AC_LEVEL01_BU, F.AC_LEVEL02_POSN, F.AC_LEVEL02_EMPLID, F.AC_EXT_LVL02_NAME, F.AC_LEVEL02_BU, F.AC_LEVEL03_POSN, F.AC_LEVEL03_EMPLID, F.AC_EXT_LVL03_NAME, F.AC_LEVEL03_BU, F.AC_LEVEL04_POSN, F.AC_LEVEL04_EMPLID, F.AC_EXT_LVL04_NAME, F.AC_LEVEL04_BU, F.AC_LEVEL05_POSN, F.AC_LEVEL05_EMPLID, F.AC_EXT_LVL05_NAME, F.AC_LEVEL05_BU, F.AC_LEVEL06_POSN, F.AC_LEVEL06_EMPLID, F.AC_EXT_LVL06_NAME, F.AC_LEVEL06_BU, F.AC_LEVEL07_POSN, F.AC_LEVEL07_EMPLID, F.AC_EXT_LVL07_NAME, F.AC_LEVEL07_BU, F.AC_LEVEL08_POSN, F.AC_LEVEL08_EMPLID, F.AC_EXT_LVL08_NAME, F.AC_LEVEL08_BU, G.AC_ACQUISITION_COD, TO_CHAR(H.JOB_ENTRY_DT,'YYYY-MM-DD'), H.FULL_PART_TIME, H.FLSA_STATUS, H.EMPL_TYPE, H.GP_PAYGROUP, TO_CHAR(H.EFFDT,'YYYY-MM-DD'), TO_CHAR(H.REHIRE_DT,'YYYY-MM-DD'), H.WORK_PHONE, H.HOME_PHONE, TO_CHAR(D.HIRE_DT,'YYYY-MM-DD'), H.GRADE, H.NATIONAL_ID, H.LAST_NAME, H.FIRST_NAME, H.PREF_FIRST_NAME, H.ANNUAL_RT, D.DEPTID, TO_CHAR(H.POSITION_ENTRY_DT,'YYYY-MM-DD'), H.SEX, H.HIGHEST_EDUC_LVL, H.MILITARY_STATUS, H.MILIT_SITUATN_FRA, H.DISABLED, H.DISABLED_VET, H.ETHNIC_GROUP, TO_CHAR(H.BIRTHDATE,'YYYY-MM-DD'), I.STATE, I.POSTAL
  FROM PS_PSTREENODE_DW A, (SELECT TD.POSITION_NBR, TD.EMPLID, TD.NAME, TD.SERVICE_DT, TD.JOBCODE, TD.JOBCODE_DESCR, TD.LOCATION, TD.LOCATION_DESCR, TD.AC_BUSINESS_UNIT, TD.AC_JDE_DESCR, TD.REPORTS_TO, TD.SUPERVISOR_ID, TD.AC_SUPER_NAME, TD.HIRE_DT, TD.DEPTID
  FROM PS_AC_NSPOS_INC_VW TD,PS_AC_POSN_SRCH D1 WHERE TD.POSITION_NBR = D1.POSITION_NBR AND  D1.OPRCLASS = 'WIN_VW' ) D, PS_AC_REPTS_TO_LVL F, PS_AC_EMAIL_TBL E, PS_WIN_EMPLOYMENT G, PS_AC_ALL_EMPLS H, PS_EMPLMT_SRCH_QRY H1, PS_LOCATION_TBL I
  WHERE ( H.EMPLID = H1.EMPLID
    AND H1.OPRID = 'E0182265'
    AND ( A.TREE_NODE_NUM >= (SELECT B.TREE_NODE_NUM
  FROM PS_PSTREENODE_DW B
  WHERE ( B.TREE_NODE = :1 ))
     AND A.TREE_NODE_NUM_END <= (SELECT C.TREE_NODE_NUM_END
  FROM PS_PSTREENODE_DW C
  WHERE ( C.TREE_NODE = :1 ))
     AND D.POSITION_NBR = A.TREE_NODE
     AND D.JOBCODE <> 'LC003'
     AND D.POSITION_NBR = F.POSITION_NBR
     AND D.EMPLID = F.EMPLID
     AND D.EMPLID =  E.EMPLID (+)
     AND D.EMPLID =  G.EMPLID (+)
     AND F.EMPLID = H.EMPLID
     AND F.POSITION_NBR = H.POSITION_NBR
     AND H.EFFDT =
        (SELECT MAX(H_ED.EFFDT) FROM PS_AC_ALL_EMPLS H_ED
        WHERE H.EMPLID = H_ED.EMPLID
          AND H_ED.EFFDT <= SYSDATE)
    AND H.EFFSEQ =
        (SELECT MAX(H_ES.EFFSEQ) FROM PS_AC_ALL_EMPLS H_ES
        WHERE H.EMPLID = H_ES.EMPLID
          AND H.EFFDT = H_ES.EFFDT)
     AND I.LOCATION = D.LOCATION
     AND I.EFFDT =
        (SELECT MAX(I_ED.EFFDT) FROM PS_LOCATION_TBL I_ED
        WHERE I.SETID = I_ED.SETID
          AND I.LOCATION = I_ED.LOCATION
          AND I_ED.EFFDT <= SYSDATE) ))
