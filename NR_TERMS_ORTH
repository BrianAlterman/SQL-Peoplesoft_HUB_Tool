SELECT A.EMPLID, A.NAME, TO_CHAR(A.HIRE_DT,'YYYY-MM-DD'), TO_CHAR(A.REHIRE_DT,'YYYY-MM-DD'), TO_CHAR(A.SERVICE_DT,'YYYY-MM-DD'), TO_CHAR(A.TERMINATION_DT,'YYYY-MM-DD'), TO_CHAR(A.LAST_DATE_WORKED,'YYYY-MM-DD'), A.JOBCODE, A.POSITION_NBR, A.EMPL_STATUS, A.ACTION, TO_CHAR(A.ACTION_DT,'YYYY-MM-DD'), A.ACTION_REASON, A.LOCATION, A.FULL_PART_TIME, A.FLSA_STATUS, A.PAYGROUP, A.EMPL_TYPE, A.SAL_ADMIN_PLAN, A.GRADE, A.REVIEW_RATING, TO_CHAR(A.REVIEW_DT,'YYYY-MM-DD'), A.ANNUAL_RT, A.JOBTITLE, A.AC_ACT_RSN_DESCR, A.LOCATION_NAME, A.AC_BUSINESS_UNIT, A.AC_SUPER_NAME, A.AC_JDE_DESCR, A.AC_RANGEPEN
  FROM PS_AC_EMPLS_ALL A, PS_AC_EMPLMT_SRC A1
  WHERE ( A.EMPLID = A1.EMPLID
    AND A.EMPL_RCD = A1.EMPL_RCD
    AND A1.OPRCLASS = 'WIN_VW'
    AND ( A.TERMINATION_DT > TO_DATE('2018-12-31','YYYY-MM-DD') ))
