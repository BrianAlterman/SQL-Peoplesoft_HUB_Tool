SELECT D.EMPLID, D.POSITION_NBR, D.NAME, E.AC_EMAIL, TO_CHAR(D.SERVICE_DT,'YYYY-MM-DD'), TO_CHAR(D.HIRE_DT,'YYYY-MM-DD'), TO_CHAR(H.REHIRE_DT,'YYYY-MM-DD'), D.AC_BUSINESS_UNIT, D.AC_JDE_DESCR, D.DEPTID, D.JOBCODE, D.JOBCODE_DESCR, D.LOCATION, D.LOCATION_DESCR, D.REPORTS_TO, D.SUPERVISOR_ID, D.AC_SUPER_NAME, F.AC_LEVEL01_POSN, F.AC_LEVEL01_EMPLID, F.AC_EXT_LVL01_NAME, F.AC_LEVEL01_BU, F.AC_LEVEL02_POSN, F.AC_LEVEL02_EMPLID, F.AC_EXT_LVL02_NAME, F.AC_LEVEL02_BU, F.AC_LEVEL03_POSN, F.AC_LEVEL03_EMPLID, F.AC_EXT_LVL03_NAME, F.AC_LEVEL03_BU, F.AC_LEVEL04_POSN, F.AC_LEVEL04_EMPLID, F.AC_EXT_LVL04_NAME, F.AC_LEVEL04_BU, F.AC_LEVEL05_POSN, F.AC_LEVEL05_EMPLID, F.AC_EXT_LVL05_NAME
  FROM PS_PSTREENODE_DW A, (SELECT TD.EMPLID, TD.POSITION_NBR, TD.NAME, TD.SERVICE_DT, TD.HIRE_DT, TD.AC_BUSINESS_UNIT, TD.AC_JDE_DESCR, TD.DEPTID, TD.JOBCODE, TD.JOBCODE_DESCR, TD.LOCATION, TD.LOCATION_DESCR, TD.REPORTS_TO, TD.SUPERVISOR_ID, TD.AC_SUPER_NAME
  FROM PS_AC_NSPOS_INC_VW TD,PS_AC_POSN_SRCH D1 WHERE TD.POSITION_NBR = D1.POSITION_NBR AND  D1.OPRCLASS = 'WIN_VW' ) D, PS_AC_REPTS_TO_LVL F, PS_AC_EMAIL_TBL E, PS_WIN_EMPLOYMENT G, PS_AC_ALL_EMPLS H, PS_EMPLMT_SRCH_QRY H1
  WHERE ( H.EMPLID = H1.EMPLID
    AND H1.OPRID = 'E0182265'
    AND ( A.TREE_NODE_NUM >= (SELECT B.TREE_NODE_NUM
  FROM PS_PSTREENODE_DW B
  WHERE ( B.TREE_NODE = :1 ))
     AND A.TREE_NODE_NUM_END <= (SELECT C.TREE_NODE_NUM_END
  FROM PS_PSTREENODE_DW C
  WHERE ( C.TREE_NODE = :1 ))
     AND D.POSITION_NBR = A.TREE_NODE
     AND D.JOBCODE <> 'LC003'
     AND D.POSITION_NBR = F.POSITION_NBR
     AND D.EMPLID = F.EMPLID
     AND D.EMPLID =  E.EMPLID (+)
     AND D.EMPLID =  G.EMPLID (+)
     AND F.EMPLID = H.EMPLID
     AND F.POSITION_NBR = H.POSITION_NBR
     AND H.EFFDT =
        (SELECT MAX(H_ED.EFFDT) FROM PS_AC_ALL_EMPLS H_ED
        WHERE H.EMPLID = H_ED.EMPLID
          AND H_ED.EFFDT <= SYSDATE)
    AND H.EFFSEQ =
        (SELECT MAX(H_ES.EFFSEQ) FROM PS_AC_ALL_EMPLS H_ES
        WHERE H.EMPLID = H_ES.EMPLID
          AND H.EFFDT = H_ES.EFFDT) ))
