SELECT A.EMPLID, TO_CHAR(A.TERMINATION_DT,'YYYY-MM-DD'), A.EMPL_STATUS, B.PHONE_TYPE, B.PHONE, C.ADDRESS1, C.ADDRESS2, C.ADDRESS3, C.ADDRESS4, C.CITY, C.STATE, C.POSTAL, D.E_ADDR_TYPE, D.EMAIL_ADDR
  FROM PS_AC_EMPLS_ALL A, PS_AC_EMPLMT_SRC A1, PS_PERSONAL_PHONE B, PS_PERALL_SEC_QRY B1, PS_ADDRESSES C, PS_EMAIL_ADDRESSES D, PS_PERALL_SEC_QRY D1
  WHERE ( A.EMPLID = A1.EMPLID
    AND A.EMPL_RCD = A1.EMPL_RCD
    AND A1.OPRCLASS = 'WIN_VW'
    AND B.EMPLID = B1.EMPLID
    AND B1.OPRID = 'E0182265'
    AND D.EMPLID = D1.EMPLID
    AND D1.OPRID = 'E0182265'
    AND ( A.EFFDT =
        (SELECT MAX(A_ED.EFFDT) FROM PS_AC_EMPLS_ALL A_ED
        WHERE A.EMPLID = A_ED.EMPLID
          AND A.EMPL_RCD = A_ED.EMPL_RCD
          AND A_ED.EFFDT <= SYSDATE)
    AND A.EFFSEQ =
        (SELECT MAX(A_ES.EFFSEQ) FROM PS_AC_EMPLS_ALL A_ES
        WHERE A.EMPLID = A_ES.EMPLID
          AND A.EMPL_RCD = A_ES.EMPL_RCD
          AND A.EFFDT = A_ES.EFFDT)
     AND A.EMPLID = B.EMPLID
     AND A.EMPLID = C.EMPLID
     AND C.EFFDT =
        (SELECT MAX(C_ED.EFFDT) FROM PS_ADDRESSES C_ED
        WHERE C.EMPLID = C_ED.EMPLID
          AND C.ADDRESS_TYPE = C_ED.ADDRESS_TYPE
          AND C_ED.EFFDT <= SYSDATE)
     AND A.EMPLID = D.EMPLID ))
