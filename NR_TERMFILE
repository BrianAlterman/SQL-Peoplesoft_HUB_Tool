	
SELECT A.EMPLID, TO_CHAR(A.TERMINATION_DT,'YYYY-MM-DD'), A.DIST_ID, A.AC_BUSINESS_UNIT, A.NATIONAL_ID, A.AC_REHIRE_IND, A.AC_VAC_HRS_DUE, A.AC_HOL_HRS_DUE, A.AC_SEV_HRS_DUE, A.AC_SEV_AMT_DUE, A.AC_FRONT_PAY_AMT, A.ACTION, A.ACTION_REASON, A.AC_ACI_VOL_TERM, A.AC_AIS_TERM_EMP, A.AC_INACTIVATE_POSN, TO_CHAR(A.AC_LAST_DT_PD_THRU,'YYYY-MM-DD'), A.AC_ILO_IND, A.AC_CHK_ATTACH_WAGE, A.AC_CHK_INSURANCE, A.DEPTID, A.DEPTNAME, B.EMPLID, B.NAME, B.LAST_NAME, B.FIRST_NAME, B.SEX, B.MAR_STATUS, TO_CHAR(B.MAR_STATUS_DT,'YYYY-MM-DD'), TO_CHAR(B.BIRTHDATE,'YYYY-MM-DD'), B.HIGHEST_EDUC_LVL, B.DISABLED_VET, B.DISABLED, B.GRADE, B.BARG_UNIT, B.MILITARY_STATUS
  FROM PS_AC_FORM_TERMS A, PS_PERSONAL_DATA B, PS_PERALL_SEC_QRY B1
  WHERE ( B.EMPLID = B1.EMPLID
    AND B1.OPRID = 'E0182265'
    AND ( B.EMPLID = A.EMPLID
     AND A.TERMINATION_DT BETWEEN TO_DATE('2016-05-01','YYYY-MM-DD') AND TO_DATE('2016-05-31','YYYY-MM-DD') ))
