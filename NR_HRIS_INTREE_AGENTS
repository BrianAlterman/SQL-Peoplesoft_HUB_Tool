SELECT E.AC_EXT_LVL01_NAME, E.AC_EXT_LVL02_NAME, E.AC_EXT_LVL03_NAME, E.AC_EXT_LVL04_NAME, E.AC_EXT_LVL05_NAME, E.AC_EXT_LVL06_NAME, D.EMPLID, D.NAME, D.POSITION_NBR, D.EMPL_STATUS, D.AC_SUPER_NAME, D.REPORTS_TO, D.JOBCODE_DESCR, D.LOCATION_DESCR, D.STD_HOURS, D.HOURLY_RT, D.ANNUAL_RT, TO_CHAR(D.SERVICE_DT,'YYYY-MM-DD'), TO_CHAR(D.HIRE_DT,'YYYY-MM-DD'), D.AC_BUSINESS_UNIT, D.AC_JDE_DESCR, D.LOCATION, D.JOBCODE, D.DIST_ID, D.AC_DESCR50, D.SUPERVISOR_ID
  FROM PS_PSTREENODE_DW A, PS_AC_POSN_SBUD_VW D, PS_AC_POSN_SRCH D1, PS_AC_REPTS_TO_LVL E
  WHERE ( D.POSITION_NBR = D1.POSITION_NBR
    AND D1.OPRCLASS = 'WIN_VW'
    AND ( A.TREE_NODE_NUM >= (SELECT B.TREE_NODE_NUM
  FROM PS_PSTREENODE_DW B
  WHERE B.TREE_NODE = :1)
     AND A.TREE_NODE_NUM_END <= (SELECT C.TREE_NODE_NUM_END
  FROM PS_PSTREENODE_DW C
  WHERE C.TREE_NODE = :1)
     AND D.POSITION_NBR = A.TREE_NODE
     AND D.POSITION_NBR = E.POSITION_NBR
     AND D.EMPLID = E.EMPLID
     AND D.JOBCODE LIKE 'LC%' ))
