	
SELECT D.EMPLID, D.POSITION_NBR, D.NAME, G.AC_EMAIL, D.JOBCODE, D.JOBCODE_DESCR, TO_CHAR(E.SERVICE_DT,'YYYY-MM-DD'), TO_CHAR(E.TERMINATION_DT,'YYYY-MM-DD'), TO_CHAR(E.REHIRE_DT,'YYYY-MM-DD'), D.AC_BUSINESS_UNIT, D.AC_JDE_DESCR, D.LOCATION, D.LOCATION_DESCR, H.POSTAL, D.AC_SUPER_NAME, F.AC_EXT_LVL00_NAME, F.AC_EXT_LVL01_NAME, F.AC_EXT_LVL02_NAME, F.AC_EXT_LVL03_NAME, F.AC_EXT_LVL04_NAME, F.AC_EXT_LVL05_NAME, F.AC_EXT_LVL06_NAME, F.AC_EXT_LVL07_NAME, F.AC_EXT_LVL08_NAME, E.WORK_PHONE
  FROM PS_PSTREENODE_DW A, PS_AC_POSN_SBUD_VW D, PS_AC_POSN_SRCH D1, PS_AC_REPTS_TO_LVL F, PS_LOCATION_TBL H, PS_AC_ALL_EMPLS E, PS_EMPLMT_SRCH_QRY E1, PS_AC_EMAIL_TBL G
  WHERE ( D.POSITION_NBR = D1.POSITION_NBR
    AND D1.OPRCLASS = 'WIN_VW'
    AND E.EMPLID = E1.EMPLID
    AND E1.OPRID = 'E0182265'
    AND ( A.TREE_NODE_NUM >= (SELECT B.TREE_NODE_NUM
  FROM PS_PSTREENODE_DW B
  WHERE B.TREE_NODE = :1)
     AND A.TREE_NODE_NUM_END <= (SELECT C.TREE_NODE_NUM_END
  FROM PS_PSTREENODE_DW C
  WHERE C.TREE_NODE = :1)
     AND D.POSITION_NBR = A.TREE_NODE
     AND D.EMPLID <> ' '
     AND D.JOBCODE <> 'LC003'
     AND D.POSITION_NBR = F.POSITION_NBR
     AND D.EMPLID = F.EMPLID
     AND H.LOCATION = D.LOCATION
     AND H.EFFDT =
        (SELECT MAX(H_ED.EFFDT) FROM PS_LOCATION_TBL H_ED
        WHERE H.SETID = H_ED.SETID
          AND H.LOCATION = H_ED.LOCATION
          AND H_ED.EFFDT <= SYSDATE)
     AND F.EMPLID = E.EMPLID
     AND F.POSITION_NBR = E.POSITION_NBR
     AND E.EFFDT =
        (SELECT MAX(E_ED.EFFDT) FROM PS_AC_ALL_EMPLS E_ED
        WHERE E.EMPLID = E_ED.EMPLID
          AND E_ED.EFFDT <= SYSDATE)
    AND E.EFFSEQ =
        (SELECT MAX(E_ES.EFFSEQ) FROM PS_AC_ALL_EMPLS E_ES
        WHERE E.EMPLID = E_ES.EMPLID
          AND E.EFFDT = E_ES.EFFDT)
     AND E.EMPLID = G.EMPLID ))
