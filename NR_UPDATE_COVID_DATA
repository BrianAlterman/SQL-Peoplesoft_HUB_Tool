SELECT A.EMPLID, A.NAME, A.JOBTITLE, A.AC_EMAIL, H.NAME_AC, A.LOCATION, A.LOCATION_NAME, B.ADDRESS1, B.CITY, B.STATE, B.POSTAL, A.AC_SUPER_NAME, D.AC_EMAIL, C.AC_LEVEL01_EMPLID, C.AC_EXT_LVL01_NAME, E.AC_EMAIL, C.AC_LEVEL02_EMPLID, C.AC_EXT_LVL02_NAME, F.AC_EMAIL, C.AC_LEVEL03_EMPLID, C.AC_EXT_LVL03_NAME, G.AC_EMAIL, A.UNION_CD, I.DANGER_OCC_CD
  FROM ((((PS_AC_EMPLS_VW A INNER JOIN PS_EMPLMT_SRCH_QRY A1 ON (A.EMPLID = A1.EMPLID AND  A1.OPRID = 'E0182265' )) LEFT OUTER JOIN  PS_AC_EMAIL_TBL D ON  A.SUPERVISOR_ID = D.EMPLID ) LEFT OUTER JOIN  PS_WIN_HRBP_EMP_VW H ON  A.EMPLID = H.EMPLID ) LEFT OUTER JOIN  PS_WIN_COVID_STAT I ON  A.EMPLID = I.EMPLID AND I.EFFDT =
        (SELECT MAX(I_ED.EFFDT) FROM PS_WIN_COVID_STAT I_ED
        WHERE I.EMPLID = I_ED.EMPLID
          AND I_ED.EFFDT <= SYSDATE) ), PS_LOCATION_TBL B, (((PS_AC_REPTS_TO_LVL C LEFT OUTER JOIN  PS_AC_EMAIL_TBL E ON  C.AC_LEVEL01_EMPLID = E.EMPLID ) LEFT OUTER JOIN  PS_AC_EMAIL_TBL F ON  C.AC_LEVEL02_EMPLID = F.EMPLID ) LEFT OUTER JOIN  PS_AC_EMAIL_TBL G ON  C.AC_LEVEL03_EMPLID = G.EMPLID )
  WHERE ( ( A.EFFDT =
        (SELECT MAX(A_ED.EFFDT) FROM PS_AC_EMPLS_VW A_ED
        WHERE A.EMPLID = A_ED.EMPLID
          AND A_ED.EFFDT <= SYSDATE)
    AND A.EFFSEQ =
        (SELECT MAX(A_ES.EFFSEQ) FROM PS_AC_EMPLS_VW A_ES
        WHERE A.EMPLID = A_ES.EMPLID
          AND A.EFFDT = A_ES.EFFDT)
     AND B.LOCATION = A.LOCATION
     AND B.EFFDT =
        (SELECT MAX(B_ED.EFFDT) FROM PS_LOCATION_TBL B_ED
        WHERE B.SETID = B_ED.SETID
          AND B.LOCATION = B_ED.LOCATION
          AND B_ED.EFFDT <= SYSDATE)
     AND A.EMPLID = C.EMPLID
     AND C.POSITION_NBR = A.POSITION_NBR ))
  ORDER BY 6
